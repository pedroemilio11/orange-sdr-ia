  "author": "Orange Incorporadora",
  "license": "PROPRIETARY",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "openai": "^4.20.0",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "rate-limiter-flexible": "^3.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "@types/jest": "^29.5.8",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
